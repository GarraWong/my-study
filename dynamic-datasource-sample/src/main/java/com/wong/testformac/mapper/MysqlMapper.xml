<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wong.testformac.mapper.MysqlMapper">


    <select id="selectList"  resultType="com.wong.testformac.domain.Name">
        SELECT * FROM myname
    </select>

    <delete id="deleteName">
        UPDATE myname SET
            name = '我改了'
        WHERE id = #{id}
    </delete>

    <insert id="insertName" parameterType="com.wong.testformac.domain.Name">
        INSERT INTO myname(name) values (#{name})

    </insert>

</mapper>